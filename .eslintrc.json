{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:i18next/recommended"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
